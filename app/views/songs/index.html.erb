<h1>Song Reviews</h1>

<% @songs.each do |song| %>

    <%= song.title %>

    <%= song.artist %>

    <%= link_to 'Show', song_path(song) %>

    <%= link_to 'Edit', edit_song_path(song) %>

    <%= link_to 'Delete', song, method: :delete %>

    
<% end %>

<%= link_to 'New Review', new_song_path %>